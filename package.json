{
  "name": "kluster-kaspa",
  "private": false,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:signing-tool": "turbo run dev --filter=signing-tool",
    "dev:kip-12-dapp": "turbo run dev --filter=kip-12-dapp",
    "dev:react-starter-kit": "turbo run dev --filter=react-starter-kit",
    "dev:node-starter-kit": "turbo run dev --filter=node-starter-kit",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {}
}
